{
  "name": "bacon-examples",
  "version": "1.0.0",
  "description": "Visual examples for Bacon.js",
  "scripts": {
    "clean": "rm -rf target",
    "build": "mkdir -p target && node_modules/.bin/browserify src/main.jsx -o target/bacon-examples.js --extension=.jsx -d -t [ babelify --sourceMaps true --extensions .jsx ]",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
